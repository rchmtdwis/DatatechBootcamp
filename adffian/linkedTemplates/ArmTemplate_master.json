{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adffian"
		},
		"ADLSFian_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSFian'"
		},
		"SQLFian_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SQLFian'"
		},
		"ADLSFian_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccountfian.dfs.core.windows.net/"
		},
		"HTTP_solliancepublicdata_Customer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://solliancepublicdata.blob.core.windows.net/dataengineering/dp-203/awdata/CustomerSource.csv"
		},
		"SQLFian_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "fian.database.windows.net"
		},
		"SQLFian_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "data"
		},
		"SQLFian_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "fian"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adffian_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSFian_accountKey": {
						"value": "[parameters('ADLSFian_accountKey')]"
					},
					"SQLFian_password": {
						"value": "[parameters('SQLFian_password')]"
					},
					"ADLSFian_properties_typeProperties_url": {
						"value": "[parameters('ADLSFian_properties_typeProperties_url')]"
					},
					"HTTP_solliancepublicdata_Customer_properties_typeProperties_url": {
						"value": "[parameters('HTTP_solliancepublicdata_Customer_properties_typeProperties_url')]"
					},
					"SQLFian_properties_typeProperties_server": {
						"value": "[parameters('SQLFian_properties_typeProperties_server')]"
					},
					"SQLFian_properties_typeProperties_database": {
						"value": "[parameters('SQLFian_properties_typeProperties_database')]"
					},
					"SQLFian_properties_typeProperties_userName": {
						"value": "[parameters('SQLFian_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}